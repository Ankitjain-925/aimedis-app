  },
  "devDependencies": {
    "dotenv-cli": "^7.0.0",
    "eslint-config-custom": "*",
    "prettier": "latest",
    "turbo": "^1.8.1"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "packageManager": "npm@9.3.1",
  "dependencies": {
    "@fontsource/poppins": "^4.5.10",
    "react-horizontal-scrolling-menu": "^3.2.5"
  }
}
